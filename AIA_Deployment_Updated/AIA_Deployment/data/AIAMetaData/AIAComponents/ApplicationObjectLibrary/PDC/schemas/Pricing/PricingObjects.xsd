    <!-- _____________________________________________________________-->
    <xs:complexType name="ComplexTriggerExpression" abstract="false">
        <xs:annotation>
            <xs:documentation>ComplexTriggerExpression class based on ATriggerExpression. Can be used to recursively
                              define complex expression using simple expressions</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:ATriggerExpression">
                <xs:sequence>
                    <xs:element name="binaryExpression" type="pdc:BinaryExpression"/>
