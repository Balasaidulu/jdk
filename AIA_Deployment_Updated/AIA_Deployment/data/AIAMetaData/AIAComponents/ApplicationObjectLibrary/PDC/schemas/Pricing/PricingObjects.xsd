                    <xs:element name="quantityTriggerExpression" type="pdc:QuantityTriggerExpression"/>
                    <xs:element name="balanceTriggerExpression" type="pdc:BalanceTriggerExpression"/>
                    <xs:element name="customTriggerExpression" type="pdc:CustomTriggerExpression"/>
                    <xs:element name="complexTriggerExpression" type="pdc:ComplexTriggerExpression"/>
