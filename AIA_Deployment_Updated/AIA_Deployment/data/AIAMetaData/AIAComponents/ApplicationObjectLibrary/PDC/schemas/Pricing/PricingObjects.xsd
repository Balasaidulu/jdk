        </xs:annotation>
        <xs:sequence>
            <xs:element name="leftOperand" type="pdc:BinaryOperandExpression"/>
            <xs:element name="rightOperand" type="pdc:BinaryOperandExpression"/>
            <xs:element name="binaryOperator" type="cmn:ArithmeticOperator" minOccurs="1"/>
        </xs:sequence>
