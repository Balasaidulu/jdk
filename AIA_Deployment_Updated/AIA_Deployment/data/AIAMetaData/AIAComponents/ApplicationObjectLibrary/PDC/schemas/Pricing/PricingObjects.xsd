        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     QuantityTriggerExpression                -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="QuantityTriggerExpression" abstract="false">
        <xs:annotation>
            <xs:documentation>QuantityTriggerExpression class based on ATriggerExpression.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:ATriggerExpression"></xs:extension>
        </xs:complexContent>
