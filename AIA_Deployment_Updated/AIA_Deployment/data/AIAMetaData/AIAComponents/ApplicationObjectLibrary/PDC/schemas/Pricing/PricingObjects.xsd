    <!-- _____________________________________________________________-->
    <!--                     BalanceTriggerExpression                 -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="BalanceTriggerExpression" abstract="false">
        <xs:annotation>
            <xs:documentation>BalanceTriggerExpression class based on ATriggerExpression.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
