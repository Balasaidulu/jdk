<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:pdc="http://xmlns.oracle.com/communications/platform/model/pricing"
           xmlns:cmn="http://xmlns.oracle.com/communications/platform/model/Common"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="1.0"
           elementFormDefault="unqualified" attributeFormDefault="unqualified"
           targetNamespace="http://xmlns.oracle.com/communications/platform/model/pricing">
    <xs:import namespace="http://xmlns.oracle.com/communications/platform/model/Common"
               schemaLocation="../common/common.xsd"/>
    <xs:annotation>
        <xs:documentation>Root for all pricing objects</xs:documentation>
        <xs:appinfo>
            <jxb:schemaBindings>
                <jxb:package name="oracle.communications.brm.pdc.server.pricing.jaxb"/>
                <jxb:nameXmlTransform>
                    <jxb:typeName suffix="JXB"/>
                </jxb:nameXmlTransform>
            </jxb:schemaBindings>
        </xs:appinfo>
    </xs:annotation>
    <!-- _____________________________________________________________-->
    <!--                  PricingObjects                              -->
    <!-- _____________________________________________________________-->
    <xs:element name="PricingObjectsJXB">
        <!-- _____________________________________________________________-->
        <!--                  PricingObjects                              -->
        <!-- _____________________________________________________________-->
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Root for all pricing objects</xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _________________________________________________________-->
                    <!--                  PriceModel                              -->
                    <!-- _________________________________________________________-->
                    <xs:element name="priceModel" type="pdc:PriceModel" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  TimeModel                                   -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="timeModel" type="pdc:TimeModel" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  Generic Selector                            -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="genericSelector" type="pdc:AttributeAnalyzerModel" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  Price Model Selector                        -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="priceModelSelector" type="pdc:AttributeAnalyzerModel" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  APN selector                                -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="apnSelector" type="pdc:AttributeAnalyzerModel" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  USC Selector                                -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="uscSelector" type="pdc:AttributeAnalyzerModel" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  TriggerSpecs                                -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="triggerSpec" type="pdc:TriggerSpec" minOccurs="0" maxOccurs="unbounded"/>
                    <!-- _____________________________________________________________-->
                    <!--                  ChargeSelectorSpecs                         -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="chargeSelectorSpec" type="pdc:ChargeSelectorSpec" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  RatePlans                                   -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="alterationRatePlan" type="pdc:AlterationRatePlan" minOccurs="0"
                                maxOccurs="unbounded"/>
                    <xs:element name="chargeRatePlan" type="pdc:ChargeRatePlan" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="distributionRatePlan" type="pdc:DistributionRatePlan" minOccurs="0"
                                maxOccurs="unbounded"/>
                    <xs:element name="rolloverRatePlan" type="pdc:RolloverRatePlan" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  CRP selector                                -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="chargeRateplanSelector" type="pdc:AttributeAnalyzerModel" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  ARP selector                                -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="alterationRateplanSelector" type="pdc:AttributeAnalyzerModel" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                     ChargeOffering                           -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="chargeOffering" type="pdc:ChargeOffering" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A ChargeOffering class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--                     AlterationOffering                       -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="alterationOffering" type="pdc:AlterationOffering" minOccurs="0"
                                maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>An AlterationOffering class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--                     DistributionOffering                     -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="distributionOffering" type="pdc:DistributionOffering" minOccurs="0"
                                maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A DistributionOffering class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                     BundledProductOffering                   -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="bundledProductOffering" type="pdc:BundledProductOffering" minOccurs="0"
                                maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A BundledProductOffering</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  Package                                     -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="package" type="pdc:Package" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A Package</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <!-- _____________________________________________________________-->
                    <!--                  PackageList                                 -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="packageList" type="pdc:PackageList" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A PackageList</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--                     BestPricing                              -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="bestPricing" type="pdc:BestPricing" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A BestPricing class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--               BundledProductOfferingDependency               -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="bundledProductOfferingDependency" type="pdc:BundledProductOfferingDependency"
                                minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A BundledProductOfferingDependency class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--                     PackageExclusion                         -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="packageExclusion" type="pdc:PackageExclusion" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A PackageExclusion class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--                 AlterationOfferingExclusion                  -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="alterationOfferingExclusion" type="pdc:AlterationOfferingExclusion" minOccurs="0"
                                maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A AlterationOfferingExclusion class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--                     PackageTransition                        -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="packageTransition" type="pdc:PackageTransition" minOccurs="0"
                                maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A PackageTransition class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- _____________________________________________________________-->
                    <!--              BundledProductOfferingTransition                -->
                    <!-- _____________________________________________________________-->
                    <xs:element name="bundledProductOfferingTransition" type="pdc:BundledProductOfferingTransition"
                                minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A BundledProductOfferingTransition class.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- _____________________________________________________________-->
    <!--                     PriceModel                               -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="PriceModel" abstract="false">
        <xs:annotation>
            <xs:documentation>PriceModel</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:APricingObject">
                <xs:sequence>
                    <xs:choice id="PopModelInPriceModel">
                        <xs:element name="alterationPopModel" type="pdc:AlterationPopModel"/>
                        <xs:element name="oneTimePopModel" type="pdc:OneTimePopModel"/>
                        <xs:element name="recurringPopModel" type="pdc:RecurringPopModel"/>
                        <xs:element name="replacementPopModel" type="pdc:ReplacementPopModel"/>
                        <xs:element name="rolloverPopModel" type="pdc:RolloverPopModel"/>
                        <xs:element name="usageChargePopModel" type="pdc:UsageChargePopModel"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     OneTimeChargePop                         -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="OneTimeChargePop" abstract="false">
        <xs:annotation>
            <xs:documentation>An OneTimeChargeProdOfferPriceCharge.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AProdOfferPriceCharge"></xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     FixedChargePop                           -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="FixedChargePop" abstract="false">
        <xs:annotation>
            <xs:documentation>A FixedProdOfferPriceCharge.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AProdOfferPriceCharge"></xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     ScaledChargePop                          -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ScaledChargePop" abstract="false">
        <xs:annotation>
            <xs:documentation>A ScaledProdOfferPriceCharge.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AProdOfferPriceCharge">
                <xs:sequence>
                    <xs:element name="incrementStep" type="xs:int" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="incrementRounding" type="cmn:IncrementRounding" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     RecurringChargePop                       -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="RecurringChargePop" abstract="false">
        <xs:annotation>
            <xs:documentation>An RecurringChargeProdOfferPriceCharge.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AProdOfferPriceCharge">
                <xs:sequence>
                    <xs:element name="proratable" type="xs:boolean"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     FixedProdOfferPriceAlteration            -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="FixedAlterationPop" abstract="false">
        <xs:annotation>
            <xs:documentation>A FixedProdOfferPriceAlteration.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AProdOfferPriceAlteration">
                <xs:sequence>
                    <xs:element name="incrementStep" type="xs:int"/>
                    <xs:element name="prorateLastIncrementStep" type="xs:boolean"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     PercentProdOfferPriceAlteration          -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="PercentAlterationPop" abstract="false">
        <xs:annotation>
            <xs:documentation>A PercentProdOfferPriceAlteration.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AProdOfferPriceAlteration"></xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     ProdOfferPriceRollover                   -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="RolloverPop" abstract="false">
        <xs:annotation>
            <xs:documentation>Rollover Pop charge definition</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="glid" type="xs:int" minOccurs="0" maxOccurs="1"/>
            <!--xs:element name="price" type="xs:double"/-->
            <xs:element name="unitOfMeasure" type="cmn:RolloverUnitOfMeasure"/>
            <xs:element name="balanceElementNumCode" type="xs:long"/>
            <xs:element name="rolloverUnits" type="xs:double"/>
            <xs:element name="rolloverMaxUnits" type="xs:double"/>
            <xs:element name="rolloverCount" type="xs:integer" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     Replacement Pop                          -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ReplacementPop" abstract="false">
        <xs:annotation>
            <xs:documentation>A ProdOfferPriceReplacement.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="glid" type="xs:int" minOccurs="0" maxOccurs="1"/>
            <xs:element name="type" type="cmn:ReplacementType"/>
            <xs:element name="taxTime" type="cmn:TaxTime" minOccurs="0" maxOccurs="1"/>
            <xs:element name="taxCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="price" type="xs:double"/>
            <xs:element name="balanceElementNumCode" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     AProdOfferPriceCharge                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AProdOfferPriceCharge" abstract="true">
        <xs:annotation>
            <xs:documentation>A ProdOfferPriceCharge.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AComponentProdOfferPrice">
                <xs:sequence>
                    <xs:element name="discountable" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="priceType" type="cmn:PriceType" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="taxTime" type="cmn:TaxTime" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="taxCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="glid" type="xs:int" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="priceValidity" type="pdc:PriceValidity" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     AProdOfferPriceAlteration                -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AProdOfferPriceAlteration" abstract="true">
        <xs:annotation>
            <xs:documentation>A ProdOfferPriceAlteration.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="pdc:AComponentProdOfferPrice">
                <xs:sequence>
                    <xs:element name="alterationAppliesTo" type="cmn:AlterationRecipientType"/>
                    <xs:element name="alterationBasedOn" type="pdc:AlterationBasisExpression"/>
                    <xs:element name="priceType" type="cmn:PriceType"/>
                    <xs:element name="taxTime" type="cmn:TaxTime" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="taxCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="glid" type="xs:int" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="priceValidity" type="pdc:PriceValidity" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  AComponentProdOfferPrice                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AComponentProdOfferPrice">
        <xs:sequence>
            <xs:element name="price" type="xs:double"/>
            <xs:element name="unitOfMeasure" type="cmn:UnitOfMeasure"/>
            <xs:element name="balanceElementNumCode" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  RumChargeTierExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="RumChargeTierExpression">
        <xs:sequence/>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  BalanceChargeTierExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="BalanceChargeTierExpression">
        <xs:sequence>
            <xs:element name="balanceElementNumCode" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  ChargeTierExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ChargeTierExpression">
        <xs:sequence>
            <xs:choice id="chargeTierExpression">
                <xs:element name="rumTierExpression" type="pdc:RumChargeTierExpression"/>
                <xs:element name="balanceTierExpression" type="pdc:BalanceChargeTierExpression"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  QuantityAlterationBasisExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="QuantityAlterationBasisExpression">
        <xs:sequence>
            <xs:element name="useTierComponent" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  BalanceAlterationBasisExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="BalanceAlterationBasisExpression">
        <xs:sequence>
            <xs:element name="balanceElementNumCode" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  ChargeAlterationBasisExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ChargeAlterationBasisExpression">
        <xs:sequence>
            <xs:element name="useTierComponent" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  CustomAlterationBasisExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="CustomAlterationBasisExpression">
        <xs:sequence>
            <xs:element name="functionName" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  NumberAlterationBasisExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="NumberAlterationBasisExpression">
        <xs:sequence>
            <xs:element name="value" type="xs:double"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  BinaryAlterationBasisExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="BinaryAlterationBasisExpression">
        <xs:sequence>
            <xs:element name="binaryOperator" type="xs:string"/>
            <xs:element name="leftOperand" type="pdc:AlterationBasisExpression"/>
            <xs:element name="rightOperand" type="pdc:AlterationBasisExpression"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  BalanceAlterationTBExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="BalanceAlterationTBExpression">
        <xs:sequence>
            <xs:element name="balanceElementNumCode" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  BinaryAlterationTBExpression                -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="BinaryAlterationTBExpression">
        <xs:sequence>
            <xs:element name="binaryOperator" type="xs:string"/>
            <xs:element name="leftOperand" type="pdc:AlterationTierBoundExpression"/>
            <xs:element name="rightOperand" type="pdc:AlterationTierBoundExpression"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  NumberAlterationTBExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="NumberAlterationTBExpression">
        <xs:sequence>
            <xs:element name="value" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  CustomAlterationTBExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="CustomAlterationTBExpression">
        <xs:sequence>
            <xs:element name="functionName" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  AlterationTierExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AlterationTierExpression">
        <xs:sequence>
            <xs:choice id="alterationTierExpression">
                <xs:element name="quantityTierExpression" type="pdc:QuantityAlterationBasisExpression"/>
                <xs:element name="balanceTierExpression" type="pdc:BalanceAlterationBasisExpression"/>
                <xs:element name="chargeTierExpression" type="pdc:ChargeAlterationBasisExpression"/>
                <xs:element name="customTierExpression" type="pdc:CustomAlterationBasisExpression"/>
                <xs:element name="binaryTierExpression" type="pdc:BinaryAlterationBasisExpression"/>
                <xs:element name="numberTierExpression" type="pdc:NumberAlterationBasisExpression"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  AlterationBasisExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AlterationBasisExpression">
        <xs:sequence>
            <xs:choice id="alterationBasisExpression">
                <xs:element name="quantityBasisExpression" type="pdc:QuantityAlterationBasisExpression"/>
                <xs:element name="balanceBasisExpression" type="pdc:BalanceAlterationBasisExpression"/>
                <xs:element name="chargeBasisExpression" type="pdc:ChargeAlterationBasisExpression"/>
                <xs:element name="customBasisExpression" type="pdc:CustomAlterationBasisExpression"/>
                <xs:element name="numberBasisExpression" type="pdc:NumberAlterationBasisExpression"/>
                <xs:element name="binaryBasisExpression" type="pdc:BinaryAlterationBasisExpression"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  AlterationTierBoundExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AlterationTierBoundExpression">
        <xs:sequence>
            <xs:choice id="alterationTBExpression">
                <xs:element name="numberTBExpression" type="pdc:NumberAlterationTBExpression"/>
                <xs:element name="balanceTBExpression" type="pdc:BalanceAlterationTBExpression"/>
                <xs:element name="customTBExpression" type="pdc:CustomAlterationTBExpression"/>
                <xs:element name="binaryTBExpression" type="pdc:BinaryAlterationTBExpression"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  NumberChargeTBExpr                          -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="NumberChargeTBExpression">
        <xs:sequence>
            <xs:element name="value" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  ChargeTierBoundExpression                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ChargeTierBoundExpression">
        <xs:sequence>
            <xs:choice id="chargeTierBoundExpression">
                <xs:element name="numberTBExpression" type="pdc:NumberChargeTBExpression"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     APriceTier                               -->
    <!-- _____________________________________________________________-->
    <!--For TierBasis- Possible values are USAGEQTY, RESOURCE(Balance Element numeric code),RUMQTY -->
    <xs:complexType name="APriceTier" abstract="true">
        <xs:annotation>
            <xs:documentation>A PriceTier.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="distributionMethod" type="cmn:DistributionMethod"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  PriceValidity                               -->
    <!-- _____________________________________________________________-->
