<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:cmn="http://xmlns.oracle.com/communications/platform/model/Common"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="1.0"
           elementFormDefault="unqualified" attributeFormDefault="unqualified"
           xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
           jaxb:extensionBindingPrefixes="xjc"
           targetNamespace="http://xmlns.oracle.com/communications/platform/model/Common">
    <xs:annotation>
        <xs:appinfo>
            <jaxb:globalBindings generateIsSetMethod="true">
                <xjc:serializable uid="12343"/>
            </jaxb:globalBindings>
            <jxb:schemaBindings>
                <jxb:package name="oracle.communications.brm.pdc.server.common.jaxb"/>
                <jxb:nameXmlTransform>
                    <jxb:typeName suffix="JXB"/>
                </jxb:nameXmlTransform>
            </jxb:schemaBindings>
        </xs:appinfo>
    </xs:annotation>
    <!-- _____________________________________________________________-->
    <!--                     AAlterationDependency                    -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AAlterationDependency" abstract="true">
        <xs:annotation>
            <xs:documentation>An AlterationDependency object.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:APricingObject">
                <xs:sequence>
                    <xs:element name="dependentOfferingName" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>The name of the dependent AlterationOffering.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     AConfigurationObject                     -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AConfigurationObject" abstract="true">
        <xs:annotation>
            <xs:documentation>A ConfigurationObject.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:ExternalIDedObject">
                <xs:sequence>
                    <xs:element name="priceListName" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _______________________________________________________________-->
    <!--             ADateRange                                         -->
    <!-- _______________________________________________________________-->
    <xs:complexType name="ADateRange" abstract="true">
        <xs:annotation>
            <xs:documentation>An abstract base class that Absolute and Reletive ranges are derived.</xs:documentation>
        </xs:annotation>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     RelativeDateRange                        -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="RelativeDateRange" abstract="false">
        <xs:annotation>
            <xs:documentation>A RelativeDateRange.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:ADateRange">
                <xs:sequence>
                    <xs:element name="startOffset" type="xs:integer"/>
                    <xs:element name="startUnit" type="cmn:UnitType"/>
                    <xs:element name="endOffset" type="xs:integer"/>
                    <xs:element name="endUnit" type="cmn:UnitType"/>
                    <xs:element name="base" type="cmn:BaseType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     AbsoluteDateRange                        -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AbsoluteDateRange" abstract="false">
        <xs:annotation>
            <xs:documentation>An AbsoluteDateRange.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:ADateRange">
                <xs:sequence>
                    <xs:element name="timeRange" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     APriceEventType                          -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="APriceEventType" abstract="true">
        <xs:annotation>
            <xs:documentation>A PriceEventType abstract class.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="eventName" type="xs:string"/>
            <xs:element name="validIfCancelled" type="xs:boolean"/>
            <xs:element name="validIfInactive" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                       APricingObject                         -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="APricingObject" abstract="true">
        <xs:annotation>
            <xs:documentation>A Base Class for reusable PricingObjectsJXB.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:ExternalIDedObject">
                <xs:sequence>
                    <xs:element name="currentStateName" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The name of current state of the PricingObject.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="changeSetName" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The name of the Change set of the PricingObject.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="publishSchemas" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Publish schemas of the PricingObject.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="pricingProfileName" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>The name of PricingProfile of the PricingObject.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="priceListName" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The name of PriceList of the PricingObject.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     APopModel                                -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="APopModel" abstract="true">
        <xs:annotation>
            <xs:documentation>An AProdOfferPriceModel.</xs:documentation>
        </xs:annotation>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     AProductOffering                         -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="AProductOffering" abstract="true">
        <xs:annotation>
            <xs:documentation>An AProductOffering abstract class.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:APricingObject">
                <xs:sequence>
                    <xs:element name="timeRange" type="xs:string"/>
                    <xs:choice id="assignedTo" minOccurs="0" maxOccurs="1">
                        <xs:element name="productSpecName" type="xs:string"/>
                        <xs:element name="customerSpecName" type="xs:string"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     ARatePlanGroup                           -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ARatePlanGroup" abstract="true">
        <xs:annotation>
            <xs:documentation>A RatePlanGroup abstract class.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:NamedObject">
                <xs:sequence>
                    <xs:element name="entityId" type="xs:long"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     ATransition                              -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ATransition" abstract="true">
        <xs:annotation>
            <xs:documentation>A Transition object.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:APricingObject">
                <xs:sequence>
                    <xs:element name="active" type="xs:boolean" default="true"/>
                    <xs:element name="timeRange" type="xs:string" default="0/INF"/>
                    <xs:element name="waivePurchaseFee" type="xs:boolean" default="false"/>
                    <xs:element name="waiveCancelFee" type="xs:boolean" default="false"/>
                    <xs:element name="customerSpecName" type="xs:string" minOccurs="0"/>
                    <xs:element name="productSpecName" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     VersionedObject                          -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="VersionedObject" abstract="true">
        <xs:annotation>
            <xs:documentation>An object has a name and a description field.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:ExternalIDedObject">
                <xs:sequence>
                    <xs:element name="version" type="xs:long" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The version of the PricingObject.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     ExternalIDedObject                       -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="ExternalIDedObject" abstract="true">
        <xs:annotation>
            <xs:documentation>An object has a name and a description field.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:InternalIDedObject">
                <xs:attribute name="externalID" use="optional" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>The external ID of this object. Used to create the cross references in case of
                                          migration.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     InternalIDedObject                              -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="InternalIDedObject" abstract="true">
        <xs:annotation>
            <xs:documentation>An object has a name and a description field.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cmn:MutableNamedObject">
                <xs:sequence>
                    <xs:element name="internalId" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The internal ID of the PricingObject.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     NamedObject                              -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="NamedObject" abstract="true">
        <xs:annotation>
            <xs:documentation>An object has a name and a description field.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     MutableNamedObject                       -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="MutableNamedObject" abstract="true">
        <xs:annotation>
            <xs:documentation>An object has a name, an optional newName and a description field.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="newName" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>New name for the Named objects.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     TimePeriod                               -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="TimePeriod" abstract="true">
        <xs:annotation>
            <xs:documentation>A time period defined by start time and end time.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="start" type="xs:dateTime"/>
            <xs:element name="end" type="xs:dateTime"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     MinimumRumQuantityRule                   -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="MinimumRumQuantityRule" abstract="false">
        <xs:annotation>
            <xs:documentation>A minimum quantity rule for associated RumConfiguration used in ApplicableRumSetRPEBranch.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="minQuantity" type="xs:double"/>
            <xs:element name="minQuantityUnit" type="cmn:RumQuantityUnit"/>
            <xs:element name="incrementQuantity" type="xs:double"/>
            <xs:element name="incrementQuantityUnit" type="cmn:RumQuantityUnit"/>
            <xs:element name="roundingMode" type="cmn:RoundingMode">
                <xs:annotation>
                    <xs:documentation>Specify how RUM quantity is rounded.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="applicableRumName" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                     Validity                                 -->
    <!-- _____________________________________________________________-->
    <xs:complexType name="Validity" abstract="false">
        <xs:annotation>
            <xs:documentation>A validity object to define offset, unit and validity modes.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="offset" type="xs:integer" minOccurs="0"/>
            <xs:element name="unit" type="cmn:ValidityOffsetUnit" minOccurs="0"/>
            <xs:element name="mode" type="cmn:ValidityMode" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- _____________________________________________________________-->
    <!--                  ApplicableChargeAndQuantity                 -->
    <!-- _____________________________________________________________-->
    <xs:simpleType name="ApplicableChargeAndQuantity">
        <xs:annotation>
            <xs:documentation>Allowed types for the ApplicableChargeAndQuantity.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="ORIGINAL_CHARGE"/>
            <xs:enumeration value="REMAINING_CHARGE"/>
            <xs:enumeration value="UNUSED_CHARGE_QUANTITY"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- _____________________________________________________________-->
    <!--                  ApplicableQuantity                          -->
    <!-- _____________________________________________________________-->
    <xs:simpleType name="ApplicableQuantity">
        <xs:annotation>
            <xs:documentation>Applicable quantity types.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="ORIGINAL"/>
            <xs:enumeration value="REMAINING"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- _____________________________________________________________-->
    <!--                  ApplyAlteration                             -->
    <!-- _____________________________________________________________-->
    <xs:simpleType name="AlterationRecipientType">
        <xs:annotation>
            <xs:documentation>Applicable quantity types.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="USER"/>
            <xs:enumeration value="PAYER"/>
            <xs:enumeration value="BOTH"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- _____________________________________________________________-->
    <!--                  AttributeType                               -->
    <!-- _____________________________________________________________-->
    <xs:simpleType name="AttributeType">
        <xs:annotation>
            <xs:documentation>Allowed types for the Attributes.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="LONG"/>
            <xs:enumeration value="STRING"/>
            <xs:enumeration value="DATE"/>
            <xs:enumeration value="DECIMAL"/>
            <xs:enumeration value="STRUCT"/>
            <xs:enumeration value="ARRAY"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- _____________________________________________________________-->
    <!--                  BaseType                                    -->
    <!-- _____________________________________________________________-->
    <xs:simpleType name="BaseType">
        <xs:annotation>
            <xs:documentation>Base type.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="PRODUCT_PURCHASE"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- _____________________________________________________________-->
    <!--                  BestPricingMode                             -->
    <!-- _____________________________________________________________-->
