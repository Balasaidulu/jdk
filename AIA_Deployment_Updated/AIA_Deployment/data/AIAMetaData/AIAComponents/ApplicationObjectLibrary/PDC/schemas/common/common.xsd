            <xs:enumeration value="HOUR"/>
            <xs:enumeration value="DAY"/>
            <xs:enumeration value="MONTH"/>
            <xs:enumeration value="CYCLE"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- _____________________________________________________________-->
