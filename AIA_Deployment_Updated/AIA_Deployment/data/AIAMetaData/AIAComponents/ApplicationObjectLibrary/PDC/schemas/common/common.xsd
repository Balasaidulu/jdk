            <xs:enumeration value="PACKAGE_OBJ"/>
            <xs:enumeration value="PACKAGE_LIST"/>
            <xs:enumeration value="ALTERATION_EXCLUSION"/>
            <xs:enumeration value="PACKAGE_EXCLUSION"/>
            <xs:enumeration value="PACKAGE_TRANSITION"/>
            <xs:enumeration value="BUNDLE_TRANSITION"/>
            <xs:enumeration value="BEST_PRICING"/>
            <xs:enumeration value="REPLACEMENT_POP_MODEL"/>
            <xs:enumeration value="BUNDLED_PRODUCT_OFFERING"/>
            <xs:enumeration value="BUNDLE_DEPENDENCY"/>
            <xs:enumeration value="APN_MAP"/>
