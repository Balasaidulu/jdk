            <xs:enumeration value="TAX_SUPPLIER"/>
            <xs:enumeration value="PROVISIONING_TAG"/>
            <xs:enumeration value="ZONE_RESULT_CONFIGURATION"/>
            <xs:enumeration value="HOLIDAY_CALENDAR"/>
            <xs:enumeration value="PROFILE_ATTRIBUTE_SPEC"/>
