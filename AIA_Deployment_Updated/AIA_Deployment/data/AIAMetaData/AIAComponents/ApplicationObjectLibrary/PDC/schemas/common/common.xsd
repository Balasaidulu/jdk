        <xs:annotation>
            <xs:documentation>Distribution methods.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <!-- <xs:enumeration value="UP_FRONT"/> -->
            <xs:enumeration value="FROM_BAL_IMPACT"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- _____________________________________________________________-->
    <!--                  EndValidityMode                             -->
    <!-- _____________________________________________________________-->
